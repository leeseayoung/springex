<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.penguin.spring.ex.datebase.repository.UsedGoodsRepository">
 
 	<select id ="selectUsedGoodsList" 
 	resultType="com.penguin.spring.ex.datebase.domain.UsedGoods">
 		SELECT
 			`id`
            , `sellerId`
            , `title`
            , `price`
            , `description`
            , `image`
            , `createdAt`
            , `updatedAt`
 		From
 			`uesd_goods`	
 	</select>
 
 
 </mapper>